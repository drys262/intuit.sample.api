{
  "name": "intuit.sample.api",
  "version": "0.0.1",
  "description": "Intuit GraphQL API",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build/",
    "dist": "npm run build && sh ./scripts/build.sh",
    "build": "npm run clean && tsc && tscpaths -p tsconfig.json -s ./src -o ./build",
    "start:dev": "npm run build && node -r dotenv/config --async-stack-traces build/index.js",
    "start": "node build/index.js",
    "test": "jest --verbose --detectOpenHandles",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "graphql:schema": "cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true ts-node scripts/schema.ts > schema.gql",
    "generate": "npm run graphql:schema && graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.2.2",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chance": "^1.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/mocha": "^7.0.2",
    "@types/mock-require": "^2.0.0",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.5.23",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.0.1",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^7.0.3",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "~2.23.0",
    "@typescript-eslint/parser": "~2.23.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chance": "^1.1.7",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^27.0.6",
    "mock-require": "^3.0.3",
    "mockdate": "^3.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.1",
    "async-exit-hook": "^2.0.1",
    "axios": "^0.21.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.7.7",
    "uuid": "^8.0.0"
  }
}
